{
  "name": "rrstd-react-universal-template",
  "version": "0.0.1",
  "description": "RRStd template for react universal render projects",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/server",
    "compile": "node build/scripts/compile",
    "compile-server": "node build/scripts/compile",
    "compile-loader": "node build/scripts/compile",
    "build": "npm run clean && cross-env NODE_ENV=production npm run compile",
    "build:all": "npm run build && npm run build:server",
    "build:dbg": "npm run clean && cross-env NODE_ENV=develop npm run compile",
    "build:server": "cross-env NODE_ENV=production npm run compile-loader",
    "build:server:dbg": "cross-env NODE_ENV=develop npm run compile-loader",
    "dev": "cross-env NODE_ENV=development node build/scripts/start",
    "start": "cross-env NODE_ENV=development babel-node src/server.js",
    "start:build": "node dist/server/server.js",
    "start:dbg": "npm run build:dbg && npm run build:server:dbg && npm run start:build",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.23.0",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "prop-types": "^15.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.0",
    "redux": "^3.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-resolver": "^1.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-pug": "^3.1.8",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-webpack-loaders-inline-exports": "^0.4.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.20.0",
    "chalk": "^1.1.3",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "figures": "^2.0.0",
    "file-loader": "^0.11.1",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^4.8.1",
    "null-loader": "^0.1.1",
    "postcss-loader": "^2.0.9",
    "postcss-rtl": "^1.1.3",
    "redbox-react": "^1.3.6",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.20.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  }
}
